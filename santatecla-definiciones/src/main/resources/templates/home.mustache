<html>

<head>
	<link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css">
	<link href="assets/css/material-dashboard.min.css?v=2.1.1" rel="stylesheet" />
	<link rel="stylesheet" type="text/css" href="assets/css/modalstyle.css" />
	<script src="assets/js/plugins/echarts.min.js"></script>
	<link rel="stylesheet" type="text/css" href="assets/css/searchbar.css" />
	<meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
	<title>Gestor de definiciones</title>
</head>

<body>
	<div class="card card-nav-tabs card-plain">
		<div class="card-header card-header-primary">
			<div class="nav-tabs-navigation float-left">
				<div class="nav-tabs-wrapper">
					<ul class="nav nav-tabs" data-tabs="tabs" id="tabList">
						<li class="nav-item">
							<a class="nav-link active" href="#">Home</a>
						</li>
					</ul>
				</div>
			</div>
			<!-- Log in modal button -->
			<button type="button" class="btn btn-primary float-right" data-toggle="modal" data-target="#loginModal">Login<i
				 class="material-icons">assignment</i></button>
		</div>
		<!-- Searchbar -->
		<div class="row">
			<div class="col-lg">
				<div class="box">
					<div class="container-1">
						<span class="icon"><i class="fa fa-search"></i></span> <!-- class="fa fa-search" -->
						<input type="search" id="search" placeholder="BÃºsqueda" />
					</div>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="col-lg">
				<div class="card">
					<div class="card-body">
						<div class="tab-content">
							<div class="tab-pane active" id="home">
								<div id="accordion" role="tablist">
									{{#chapters}}
									<div class="card card-collapse">
										<div class="card-header card-header-warning" role="tab" id="{{chapterName}}">
											<h5 class = "mb-0 card-title">
												<a class="collapsed" data-toggle="collapse" href="#collapse{{chapterName}}" aria-expanded="false" aria-controls="collapse{{chapterName}}">
													<i class="material-icons">keyboard_arrow_right</i>
													{{chapterName}}
												</a> 
											</h5>
										</div>
										<div id="collapse{{chapterName}}" class="collapse" role="tabpanel" aria-labelledby="{{chapterName}}" data-parent="#accordion">
											<div class="card-body">
												{{#concepts}}
												<ul>
													<li><a href="/concept">{{conceptName}}</a></li>
                                                </ul>
                                                {{/concepts}}
                                                {{#docente}}
													<div class="addQuestionSeeDiagram">
														<button type="button" class="btn btn-primary btn-fab btn-fab-mini btn-round" data-toggle="modal" data-target="#exampleModal">
			  												<i class="material-icons">add</i>
														</button>
													</div>
												{{/docente}}
                                            </div>
										</div>
									</div>
									<br />
									{{/chapters}}
									{{^chapters}}
									<span>No hay temas disponibles.</span>
									{{/chapters}}
									{{#docente}}
										<div class="addQuestionSeeDiagram">
											<button type="button" class="btn btn-primary btn-fab btn-fab-mini btn-round" data-toggle="modal" data-target="#exampleModal">
  												<i class="material-icons">add</i>
											</button>
										</div>
									{{/docente}}
								</div>
							</div>
						</div>
						<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#diagramModal">Diagrama</button>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div class="modal fade" id="diagramModal" tabindex="-1" role="dialog" aria-labelledby="diagramModalLabel" aria-hidden="true">
		<div class="modal-dialog modal-diagram">
			<div class="modal-content">
				<div class="modal-header">
					<h5 class="modal-title" id="diagramModalLabel">Diagrama de resultados</h5>
				</div>
				<div class="modal-body">
					<div id="diagram" style="height:400px; width: 400px"></div>
				</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
				</div>
			</div>
		</div>
	</div>
	<!-- change class button to "nav-link active" when move into the nav  -->
	<div class="modal fade" id="loginModal" tabindex="-1" role="dialog" aria-labelledby="diagramModalLabel" aria-hidden="true">
		<div class="modal-dialog modal-login" role="document">
			<div class="modal-content">
				<div class="card card-signup card-plain">
					<div class="modal-header">
						<div class="card-header card-header-primary">
							<button id="btnLogIn" type="button" class="close" data-dismiss="modal" aria-hidden="true">
								<i class="material-icons">clear</i>
							</button>
							<h4 class="card-title">Log in</h4>
						</div>
					</div>
					<div class="modal-body">
						<form class="form" method="" action="">
							<p class="description text-center"></p>
							<div class="card-body">
								<div class="form-group bmd-form-group">
									<div class="input-group">
										<div class="input-group-prepend">
											<div class="input-group-text"><i class="material-icons">face</i></div>
										</div>
										<input type="text" class="form-control" placeholder="Nombre...">
									</div>
								</div>
								<div class="form-group bmd-form-group">
									<div class="input-group">
										<div class="input-group-prepend">
											<div class="input-group-text"><i class="material-icons">lock_outline</i></div>
										</div>
										<input type="password" placeholder="Contrase&ntilde;a..." class="form-control">
									</div>
								</div>
							</div>
						</form>
					</div>
					<div class="modal-footer justify-content-center">
						<a href="#pablo" class="btn btn-primary btn-link btn-wd btn-lg">Empieza</a>
					</div>
				</div>
			</div>
		</div>
	</div>
	<script src="assets/js/diagramdummy.js" type="text/javascript"></script>

	<script src="assets/js/core/jquery.min.js" type="text/javascript"></script>
	<script src="assets/js/core/popper.min.js" type="text/javascript"></script>
	<script src="assets/js/core/bootstrap-material-design.min.js" type="text/javascript"></script>
	<script src="assets/js/plugins/perfect-scrollbar.jquery.min.js"></script>
	<script src="assets/js/plugins/bootstrap-notify.js"></script>
	<script src="assets/js/material-dashboard.js?v=2.1.1" type="text/javascript"></script>
</body>

</html>
